<tr
	xmlns:tal="http://xml.zope.org/namespaces/tal"
	xmlns:i18n="http://xml.zope.org/namespaces/i18n">

	<tal:block
	    tal:define="id here/getId; 
		title here/getTitle;
		state here/getState"
		>
		
	<!--
		tal:attributes="id string:row-prediction-${id}"
		class="draggable"
	-->
	<!--
	<td class="draggingHook"><img src="drag.gif" alt="sort" /></td>
	-->
	<td tal:attributes="class state; id here/getId">
		<span style="display:none" tal:content="state" />
		<img width="16" height="16"
			tal:define="suffix python:(state=='queued' or state=='running') and 'gif' or 'png'"
			tal:attributes="src string:++resource++plone4bio.images/icon_${state}.${suffix}; alt state; title state" />
	</td>
	<td width="90%">
		<a href="#">
			<img class="bdclose" alt="expand"
				tal:attributes="src context/++resource++plone4bio.images/close.gif;
					name id;
					id string:toggledetails_${id}" />
							</a>  
							<a tal:attributes="href here/absolute_url;
										id string:link_${id};
										name id"
									tal:content="title" />
							<div tal:attributes="id string:details_${id};" />
						</td>
						<!--
						<td>
					    	<img tal:attributes="id string:img_${id};
									src string:${here/absolute_url}/getShortImage;" />
						</td>
						-->
						<td class="nobr">
							<div tal:content="structure here/plone4bio.base.actions" />
	</td>
	</tal:block>
</tr>
