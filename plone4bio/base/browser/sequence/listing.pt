<div 
	 xmlns:tal="http://xml.zope.org/namespaces/tal"
	 xmlns:i18n="http://xml.zope.org/namespaces/i18n"
	 id="predictions-summary">
	<tal:block
			tal:define="predictions python:here.getFolderContents(full_objects =True);"
			tal:condition="python:len(predictions)>0">
		<table class="listing">
			<thead>
	   			<tr>
					<th>state</th>
					<th>title</th>
					<th class="nosort">actions</th>
				</tr>  
			</thead>
	        <tbody>
				<tal:prediction repeat="prediction predictions">
					<tal:row tal:content="structure prediction/plone4bio.base.listingrow" />				
				</tal:prediction>
			</tbody>
		</table>
	</tal:block>
</div>
