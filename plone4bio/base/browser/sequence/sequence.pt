<html xmlns="http://www.w3.org/1999/xhtml" 
	xmlns:tal="http://xml.zope.org/namespaces/tal"
	xmlns:metal="http://xml.zope.org/namespaces/metal"
	xmlns:i18n="http://xml.zope.org/namespaces/i18n"
	metal:use-macro="here/main_template/macros/master"
	i18n:domain="plone4bio">
<body>
<div metal:fill-slot="body"
  	tal:define="predictions python:here.getFolderContents(full_objects =True)">
		  
	<div tal:content="structure here/plone4bio.sequence.summary" />

    <div id="bdmsite-form">
		<form name="add_predictions"
	        		  method="post"
	        		  action=""
		              tal:attributes="action string:${context/absolute_url}/@@addpredictions">
	    	<div tal:replace="structure provider:plone4bio.sequenceform" />
	        <input value="Add predictions"
	        			class="standalone"
                   		id="submit-predictions"
    	               	type="submit"  />
	    </form>
	</div>

	<div id="predictions-shortchart" tal:content="structure here/plone4bio.sequence.shortchart" />

	<div tal:content="structure here/plone4bio.sequence.listing" />

</div>
</body>
</html>